<?xml version="1.0" encoding="UTF-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
    <id value="us-ph-document-bundle" />
    <url value="http://hl7.org/fhir/us/ph-library/StructureDefinition/us-ph-document-bundle" />
    <name value="USPublicHealthDocumentBundle" />
    <title value="US Public Health Document Bundle" />
    <status value="active" />
    <experimental value="false" />
    <publisher value="{site.data.fhir.ig.publisher}" />
    <contact>
        <name value="HL7 Public Health Work Group" />
        <telecom>
            <system value="url" />
            <value value="http://hl7.org/Special/committees/pher" />
        </telecom>
    </contact>
    <description value="This Bundle profile represents a public health Document Bundle. It contains a [US Public Health Composition](StructureDefinition-us-ph-composition.html)." />
    <jurisdiction>
        <coding>
            <system value="urn:iso:std:iso:3166" />
            <code value="US" />
            <display value="United States of America" />
        </coding>
        <text value="United States of America" />
    </jurisdiction>
    <fhirVersion value="4.0.1" />
    <kind value="resource" />
    <abstract value="false" />
    <type value="Bundle" />
    <baseDefinition value="http://hl7.org/fhir/StructureDefinition/Bundle" />
    <derivation value="constraint" />
    <differential>
        <element id="Bundle">
            <path value="Bundle" />
            <short value="US Public Health Document Bundle" />
        </element>
        <element id="Bundle.identifier">
            <path value="Bundle.identifier" />
            <short value="Document Bundle identifier (equivalent to ClinicalDocument.id in CDA)" />
            <definition
                value="Document Bundle identifier. This is equivalent to ClinicalDocument.id in CDA. The identifier of the contained Composition is equivalent to ClinicalDocument.setID in CDA and Composition.extensionVersionNumber is equivalent to ClinicalDocument.versionNumber in CDA." />
            <min value="1" />
            <mustSupport value="true" />
        </element>
        <element id="Bundle.type">
            <path value="Bundle.type" />
            <fixedCode value="document" />
            <mustSupport value="true" />
        </element>
        <element id="Bundle.entry">
            <path value="Bundle.entry" />
            <slicing>
                <discriminator>
                    <type value="profile" />
                    <path value="$this.resource" />
                </discriminator>
                <rules value="open" />
            </slicing>
            <min value="1" />
            <mustSupport value="true" />
        </element>
        <element id="Bundle.entry:USPublicHealthComposition">
            <path value="Bundle.entry" />
            <sliceName value="USPublicHealthComposition" />
            <min value="1" />
            <max value="1" />
            <mustSupport value="true" />
        </element>
        <element id="Bundle.entry:USPublicHealthComposition.resource">
            <path value="Bundle.entry.resource" />
            <min value="1" />
            <type>
                <code value="Resource" />
                <profile value="http://hl7.org/fhir/us/ph-library/StructureDefinition/us-ph-composition" />
            </type>
            <mustSupport value="true" />
        </element>
    </differential>
</StructureDefinition>
